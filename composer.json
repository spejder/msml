{
    "name": "spejder/msml",
    "type": "library",
    "description": "Medlemsservice til Mailingliste-synkronisering",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "symfony/yaml": "^5.0",
        "symfony/dependency-injection": "^5",
        "symfony/config": "^5",
        "symfony/console": "^4.2",
        "stecman/symfony-console-completion": "^0.11.0",
        "fduch/netrc": "^2.0",
        "spejder/odoo-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MSML\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "bin": [
        "msml"
    ],
    "require-dev": {
        "humbug/box": "^3.5",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.14",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-symfony": "^0.12.41",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "sort-packages": true,
        "lock": false
    }
}
