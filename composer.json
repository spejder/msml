{
    "name": "spejder/msml",
    "type": "library",
    "description": "Medlemsservice til Mailingliste-synkronisering",
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/acquia/netrc.git"
        }
    ],
    "require": {
        "php": ">=8.4",
        "fduch/netrc": "^2.0",
        "google/apiclient": "^2.17",
        "mmucklo/email-parse": "^2.2.1",
        "sebastian/diff": "^5",
        "spejder/odoo-client": "^1.0",
        "stecman/symfony-console-completion": "^0.14",
        "symfony/config": "^7.2 || ^8.0",
        "symfony/console": "^7.2",
        "symfony/dependency-injection": "^7.2 || ^8.0",
        "symfony/filesystem": "^7.2",
        "symfony/yaml": "^7.2 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "MSML\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "bin": [
        "msml"
    ],
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "humbug/box": "^4.6",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1",
        "phpstan/phpstan-deprecation-rules": "^1",
        "phpstan/phpstan-symfony": "^1",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "Directory"
        ]
    },
    "config": {
        "platform": {
            "php": "8.4"
        },
        "sort-packages": true,
        "lock": false,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
